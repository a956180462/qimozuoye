<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fragment_first"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".FirstFragment">

    <Button
        android:id="@+id/button_first7"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:text="退出账户"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/button_first5"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button_first5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.779" />

    <Button
        android:id="@+id/button_first3"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:text="出账登记"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_first2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.879"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_first2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_first2"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:text="进账登记"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_first1"
        app:layout_constraintEnd_toStartOf="@+id/button_first3"
        app:layout_constraintStart_toEndOf="@+id/button_first1"
        app:layout_constraintTop_toTopOf="@+id/button_first1"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/button_first1"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:text="账户信息"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.148"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.192" />

    <Button
        android:id="@+id/button_first4"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:text="进账总览"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/button_first7"
        app:layout_constraintEnd_toEndOf="@+id/button_first1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/button_first1"
        app:layout_constraintTop_toBottomOf="@+id/button_first1"
        app:layout_constraintVertical_bias="0.544" />

    <Button
        android:id="@+id/button_first6"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:text="账目统计"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_first4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.879"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/button_first4"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/button_first5"
        android:layout_width="88dp"
        android:layout_height="88dp"
        android:text="出账总览"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="@+id/button_first4"
        app:layout_constraintEnd_toStartOf="@+id/button_first6"
        app:layout_constraintStart_toEndOf="@+id/button_first4"
        app:layout_constraintTop_toTopOf="@+id/button_first4"
        app:layout_constraintVertical_bias="0.0" />

</androidx.constraintlayout.widget.ConstraintLayout>